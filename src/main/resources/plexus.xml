<plexus>
	<components>
		<component>
			<role>org.codehaus.plexus.logging.LoggerManager</role>
			<implementation>sbt.osgi.manager.tycho.LoggerManager</implementation>
			<instantiation-strategy>
				singleton
			</instantiation-strategy>
			<isolated-realm>false</isolated-realm>
			<configuration>
				<threshold>DEBUG</threshold>
			</configuration>
		</component>
		<component>
			<role>org.eclipse.tycho.dev.DevWorkspaceResolver</role>
			<implementation>
				org.eclipse.tycho.dev.DevWorkspaceResolver
			</implementation>
			<instantiation-strategy>
				singleton
			</instantiation-strategy>
		</component>
	</components>
</plexus>
